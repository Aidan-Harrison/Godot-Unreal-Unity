; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="AssetScatterer"
config/icon="res://icon.png"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=2560
window/size/height=1440
window/size/resizable=false
window/size/fullscreen=true

[input]

Start={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":83,"unicode":0,"echo":false,"script":null)
 ]
}
Reset={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":82,"unicode":0,"echo":false,"script":null)
 ]
}

[physics]

common/physics_fps=1000
common/enable_pause_aware_picking=true

[rendering]

quality/directional_shadow/size=1024
quality/shadow_atlas/size=1024
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/filters/anisotropic_filter_level=1
quality/spatial_partitioning/render_tree_balance=1.0
environment/default_environment="res://default_env.tres"
